{
    "env": {
        "browser": true,
        "es2021": true
    },
    "ignorePatterns": ["node_modules", "dist", "build"],    
    "extends": [              
        "airbnb",
        "airbnb/hooks",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
          },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-param-reassign": 0
    }
}
